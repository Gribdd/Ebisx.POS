<?xml version="1.0" encoding="utf-8" ?>
<toolkit:Popup
    x:Class="Ebisx.POS.Presentation.Views.Popups.CashPaymentPopup"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
    xmlns:vm="clr-namespace:Ebisx.POS.Presentation.ViewModels.Popup"
    x:DataType="vm:CashPaymentPopupViewModel"
    CanBeDismissedByTappingOutsideOfPopup="False"
    HorizontalOptions="Start">
    <VerticalStackLayout
        Margin="100,0"
        HeightRequest="700"
        WidthRequest="300">
        <Label
            HorizontalOptions="Center"
            Text="Welcome to .NET MAUI! CASHHHH"
            VerticalOptions="Center" />
        <Button Command="{Binding BackCommand}" Text="Go Back" />
    </VerticalStackLayout>
</toolkit:Popup>